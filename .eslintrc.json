{
  "parser": "babel-eslint",  
	"extends": "airbnb",
	"env": {
    "browser": true,
    "jest": true
  },
  "rules": {
    "import/prefer-default-export": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-underscore-dangle": [2, { "allowAfterThis": true }]  }
}
